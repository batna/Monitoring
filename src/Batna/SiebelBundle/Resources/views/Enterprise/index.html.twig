{% extends 'BatnaSiebelBundle::layout.html.twig' %}

{%  block maintitle %}{{ 'es.esManagement'|trans([], 'BatnaSiebelBundle') }}{% endblock %}

{% block corps %}

<div class="container_12">
	<div class="grid_12">
		<div class="block-border">
			<div class="block-header">
				<h1>{{ 'es.esList'|trans([], 'BatnaSiebelBundle') }}</h1><span></span>
			</div>
			<div class="block-content">
			{% if entities is empty %}
			<p>{{ 'es.noes'|trans([], 'BatnaSiebelBundle') }}</p>
			{% else %}
				<table style="margin:10px;">
				    <thead>
				        <tr>
				            <th>{{ 'es.Actions'|trans([], 'BatnaSiebelBundle') }}</th>
				            <th>{{ 'es.Name'|trans([], 'BatnaSiebelBundle') }}</th>
				            <th>{{ 'es.Fullname'|trans([], 'BatnaSiebelBundle') }}</th>
				            <th>{{ 'es.Description'|trans([], 'BatnaSiebelBundle') }}</th>
				            <th>{{ 'es.Signature'|trans([], 'BatnaSiebelBundle') }}</th>
				            <th>{{ 'es.Enablestate'|trans([], 'BatnaSiebelBundle') }}</th>
				            <th>{{ 'es.Objectid'|trans([], 'BatnaSiebelBundle') }}</th>
				            <th>{{ 'es.Version'|trans([], 'BatnaSiebelBundle') }}</th>
				            <th>{{ 'es.Attrdatabaseplatform'|trans([], 'BatnaSiebelBundle') }}</th>
				            <th>{{ 'es.Attrusemsstored'|trans([], 'BatnaSiebelBundle') }}</th>
				            <th>{{ 'es.Attrdescription'|trans([], 'BatnaSiebelBundle') }}</th>
				            <th>{{ 'es.Attrdatabaseconstr'|trans([], 'BatnaSiebelBundle') }}</th>
				            <th>{{ 'es.Serversequence'|trans([], 'BatnaSiebelBundle') }}</th>
				            <th>{{ 'es.Componentgroupsequence'|trans([], 'BatnaSiebelBundle') }}</th>
				            <th>{{ 'es.Componentsequence'|trans([], 'BatnaSiebelBundle') }}</th>
				            <th>{{ 'es.Namedsubsystemsequence'|trans([], 'BatnaSiebelBundle') }}</th>
				        </tr>
				    </thead>
				    <tbody>
				    {% for entity in entities %}
				        <tr>
				            <td>
				            	<a href="{{ path('es_show', { 'id': entity.id }) }}"><img src="{{ asset('img/icons/packs/fugue/16x16/magnifier.png') }}" /></a>
				                <a href="{{ path('es_edit', { 'id': entity.id }) }}"><img src="{{ asset('img/icons/packs/fugue/16x16/pencil.png') }}" /></a>
				          	</td>
				            <td>{{ entity.name }}</td>
				            <td>{{ entity.fullName }}</td>
				            <td>{{ entity.description }}</td>
				            <td>{{ entity.signature }}</td>
				            <td>{{ entity.enableState }}</td>
				            <td>{{ entity.objectId }}</td>
				            <td>{{ entity.version }}</td>
				            <td>{{ entity.databasePlatform }}</td>
				            <td>{{ entity.useMSstored }}</td>
				            <td>{{ entity.attrDescription }}</td>
				            <td>{{ entity.databaseConStr }}</td>
				            <td>{{ entity.serverSequence }}</td>
				            <td>{{ entity.ComponentGroupSequence }}</td>
				            <td>{{ entity.ComponentSequence }}</td>
				            <td>{{ entity.namedSubsystemSequence }}</td>
				        </tr>
				    {% endfor %}
				    </tbody>
				</table>
				{% endif %}
				<div class="block-actions">
					<ul class="actions-left">
						<li><a href="{{ path('es_new') }}" class="button">{{ 'es.newes'|trans([], 'BatnaSiebelBundle') }}</a></li>
					</ul>
				</div>
			</div>
		</div>
	</div>
</div>

{% endblock %}