<h1>Seuil list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Debut</th>
            <th>Fin</th>
            <th>Unite</th>
            <th>Severite</th>
            <th>Titre</th>
            <th>Message</th>
            <th>Variables</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('seuil_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.debut }}</td>
            <td>{{ entity.fin }}</td>
            <td>{{ entity.unite }}</td>
            <td>{{ entity.severite }}</td>
            <td>{{ entity.titre }}</td>
            <td>{{ entity.message }}</td>
            <td>{{ entity.variables }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('seuil_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('seuil_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('seuil_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
